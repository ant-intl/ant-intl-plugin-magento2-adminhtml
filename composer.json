{
    "name": "antom-magento/magento2-adminhtml",
    "description": "Antom payments admin module for magento 2",
    "require": {
        "antom-magento/magento2-core": "dev-dev-magento-2.4.2",
        "magento/framework": "^102.0|^103.0",
        "magento/module-backend": "^101.0|^102.0",
        "magento/module-config": "^101.2|^102.0",
        "magento/module-customer": "^102.0|^103.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "php-http/mock-client": "^1.3",
        "magento/magento-coding-standard": "^31.0",
        "object-calisthenics/phpcs-calisthenics-rules": "^3.5",
        "phpstan/phpstan": "^1.10"
    },
    "type": "magento2-module",
    "license": [
        "MIT"
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Antom\\Adminhtml\\": ""
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --configuration phpunit.xml.dist",
        "test-coverage": [
            "mkdir -p build/coverage",
            "vendor/bin/phpunit --configuration phpunit.xml.dist --coverage-html build/coverage"
        ]
    }
}
